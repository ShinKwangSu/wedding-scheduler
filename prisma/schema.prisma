generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Venue {
  id       Int    @id @default(autoincrement())
  name     String
  location String
  tel      String
  price    Int
  buffet   Int
}
